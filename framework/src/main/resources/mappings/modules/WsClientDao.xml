<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yqjr.modules.wsclient.dao.WsClientDao">
	<resultMap id="BaseResultMap" type="com.yqjr.modules.wsclient.entity.WsClient">
		<result column="ID" jdbcType="DECIMAL" property="id" />
		<result column="SERVICECODE" jdbcType="VARCHAR" property="serviceCode" />
		<result column="URL" jdbcType="VARCHAR" property="url" />
		<result column="TIMEOUT" jdbcType="DECIMAL" property="timeout" />
		<result column="TRANSTYPE" jdbcType="VARCHAR" property="transType" />
		<result column="SERVICENAME" jdbcType="VARCHAR" property="serviceName" />
	</resultMap>
	<sql id="Base_Column_List">
		ID, SERVICECODE, URL, TIMEOUT, TRANSTYPE, SERVICENAME
	</sql>
	<sql id="Base_Where_Condition">
		<where>
			<if test="id != null">
				and ID = #{id,jdbcType=DECIMAL}
			</if>
			<if test="serviceCode != null">
				and SERVICECODE = #{serviceCode,jdbcType=VARCHAR}
			</if>
			<if test="url != null">
				and URL = #{url,jdbcType=VARCHAR}
			</if>
			<if test="timeout != null">
				and TIMEOUT = #{timeout,jdbcType=DECIMAL}
			</if>
			<if test="transType != null">
				and TRANSTYPE = #{transType,jdbcType=VARCHAR}
			</if>
			<if test="serviceName != null">
				and SERVICENAME = #{serviceName,jdbcType=VARCHAR}
			</if>
		</where>
	</sql>
	<select id="get" parameterType="com.yqjr.modules.wsclient.entity.WsClient"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from FMK_WSCLIENT
		<if test="_parameter != null">
			<include refid="Base_Where_Condition" />
		</if>
	</select>
</mapper>